<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE node PUBLIC
  "-//freedesktop//DTD D-Bus Object Introspection 1.0//EN"
  "http://standards.freedesktop.org/dbus/1.0/introspect.dtd">
<node name="/com/nokia/NonGraphicFeedback1">
    <interface name="com.nokia.NonGraphicFeedback1">
        <method name="Play">
            <arg name="event" type="s" direction="in"/>
            <arg name="properties" type="a(sv)"/>
            <arg name="event_id" type="u" direction="out"/>
            <!-- Some Qt versions require In1, some Out1 -->
            <annotation name="org.qtproject.QtDBus.QtTypeName.In1" value="QVariantMap"/>
            <annotation name="org.qtproject.QtDBus.QtTypeName.Out1" value="QVariantMap"/>
        </method>
        <method name="Stop">
            <arg name="event_id" type="u" direction="in"/>
            <arg name="result" type="u" direction="out"/>
        </method>
    </interface>
</node>
